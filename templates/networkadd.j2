networkadd:

{% for entry in configxls.ansible_facts.spreadsheet_NetworkAddresses %}
    - addressgroupname: "{{ entry.Name }}" 
      addressgroupip: "{{ entry.Ipaddress }}"
      devicegroup: "{{ entry.Devicegroup }}"
      description: "{{ entry.Description }}"
      operation: "{{ entry.Operation }}"
      tags: "{{ entry.Tags }}"

{% endfor %}